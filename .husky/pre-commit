echo "=== Typecheck  ==="
npm run typecheck || exit 1

echo "=== Lint fix ==="
npm run lint:fix || exit 1

echo "=== Format EErrorCode  ==="
npm run enum:convert:error-response

export RUN_FULL_TEST=true
echo "set RUN_FULL_TEST=TRUE"

npm run test || exit 1

npm run dotenv:pull
npm run dotenv:push

git add .